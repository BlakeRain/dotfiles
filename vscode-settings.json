{
  "workbench.startupEditor": "none",
  "editor.fontSize": 13,
  "editor.tabSize": 2,
  "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.renderWhitespace": "all",
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.codeLens.enabled": false,
  "indentRainbow.ignoreErrorLanguages": ["*"],
  "editor.rulers": [96],
  "files.associations": {
    "*.hs": "haskell"
  },
  "files.autoSave": "off",
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "breadcrumbs.enabled": true,
  "doxdocgen.generic.authorTag": "@author Blake Rain <blake.rain@inchora.com>",
  "editor.formatOnSave": true,
  "highlight.regexes": {
    "(// ?TODO:)(.*)": [
      {
        "overviewRulerColor": "#ffcc00",
        "backgroundColor": "#ffcc00",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#d9ad00",
        "color": "#1f1f1f"
      }
    ],
    "(// ?(?:NOTE|IDEA):)(.*)": [
      {
        "overviewRulerColor": "#cc00cc",
        "backgroundColor": "#cc00cc",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#F29DF2",
        "color": "#1f1f1f"
      }
    ],
    "(// ?(?:OPTIMIZE|REVIEW):)(.*)": [
      {
        "overviewRulerColor": "#00ccff",
        "backgroundColor": "#00ccff",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#43A3BB",
        "color": "#1f1f1f"
      }
    ],
    "(// ?(?:FIX|FIXME|BUG|UGLY|HACK):)(.*)": [
      {
        "overviewRulerColor": "#cc0000",
        "backgroundColor": "#cc0000",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#ef2929",
        "color": "#1f1f1f"
      }
    ]
  },
  "editor.formatOnPaste": true,
  "projects.checkDirty": true,
  "projects.checkPaths": true,
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontLigatures": true,
  "clang-format.language.javascript.enable": false,
  "clang-format.language.typescript.enable": false,
  "cSpell.language": "en-GB",
  "git.enableSmartCommit": true,
  // Custom tags for the parser to use
  "yaml.customTags": [
    "!And",
    "!If",
    "!Not",
    "!Equals",
    "!Or",
    "!FindInMap",
    "!Base64",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!Select",
    "!Split",
    "!Join"
  ],
  "window.restoreWindows": "none",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "go",
    "handlebars",
    "html",
    "jade",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pub",
    "python",
    "restructuredtext",
    "rust",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yml"
  ],
  "workbench.editor.highlightModifiedTabs": true,
  "terminal.integrated.rendererType": "canvas",
  "doxdocgen.generic.boolReturnsTrueFalse": false,
  "doxdocgen.generic.includeTypeAtReturn": false,
  "doxdocgen.generic.returnTemplate": "",
  "doxdocgen.generic.splitCasingSmartText": false,
  "doxdocgen.file.copyrightTag": [],
  "doxdocgen.file.fileOrder": ["file", "brief"],
  "editor.detectIndentation": false,
  "C_Cpp.loggingLevel": "Warning",
  "terminal.integrated.scrollback": 5000,
  "liveshare.featureSet": "insiders",
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "docker.groupImagesBy": "None",
  "[asm]": {},
  "terminal.integrated.fontFamily": "Hack",
  "terminal.integrated.fontSize": 13,
  "prettier.jsxBracketSameLine": true,
  "prettier.printWidth": 96,
  "prettier.proseWrap": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "projects.configPath": "~/.vscode_projects.json",
  "window.titleBarStyle": "custom",
  "data.preview.theme": ".dark",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.includeRegExpList": ["CStyleComment"],
  "cSpell.diagnosticLevel": "Hint",
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.untrackedChanges": "separate",
  "workbench.editor.revealIfOpen": true,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.suggest.localityBonus": true,
  "bookmarks.navigateThroughAllFiles": true,
  "bookmarks.sideBar.expanded": true,
  "bookmarks.useWorkaroundForFormatters": true,
  "diffEditor.renderSideBySide": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "doxdocgen.generic.generateSmartText": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.packageManager": "yarn",
  "prettier.resolveGlobalModules": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "search.enableSearchEditorPreview": true,
  "cmake.configureOnOpen": false,
  "rust.mode": "rls",
  "rust.rustup": {
    "toolchain": "stable-x86_64-unknown-linux-gnu",
    "nightlyToolchain": "nightly-x86_64-unknown-linux-gnu"
  },
  "rust.rls": {
    "useRustfmt": true
  },
  "python.defaultInterpreterPath": "/usr/bin/python3",
  "python.showStartPage": false,
  "terminal.integrated.localEchoLatencyThreshold": 0,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter-notebook",
      "filenamePattern": "*.ipynb"
    }
  ],
"git.confirmSync": false,
  "settingsSync.ignoredSettings": ["window.zoomLevel"],
  "advancedNewFile.convenienceOptions": ["current", "last", "root"],
  "python.languageServer": "Pylance",
  "python.pythonPath": "/opt/homebrew/bin/python3",
  "window.zoomLevel": 1
}